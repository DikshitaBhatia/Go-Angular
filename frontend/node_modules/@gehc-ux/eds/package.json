{
    "name": "@gehc-ux/eds",
    "version": "1.14.13",
    "description": "ELEMENTS Design System",
    "license": "SEE LICENSE IN licensing",
    "repository":     {
        "type": "git",
        "url": "https://github.build.ge.com/gehc-ux/eds"
    },
    "scripts":     {
        "copy-css": "cp ./eds.min.css ./dist/eds.min.css",
        "build-scss": "node ./node_modules/@gehc-ux/eds-tools/build-css.js --src src/app/themes/main.scss --dest eds.min.css --map eds.min.css.map",
        "build-scss-only": "node ./node_modules/@gehc-ux/eds-tools/build-css.js --src src/app/themes/css-only.scss --dest css-only/eds.min.css --map css-only/eds.min.css.map",
        "build-scss-dark-theme-vars": "node src/scripts/generate-force-dark-vars.js",
        "build-docs": "node src/scripts/build-doc.js",
        "gzip": "gzip -c dist/eds-es2015.js > dist/eds-es2015.js.gz && gzip -c dist/eds-es5.js > dist/eds-es5.js.gz",
        "prebuild-pnc": "npm run gen-components-meta",
        "generate-colormap-and-theme-vars": "npm run test:generate-colormap && npm run test:generate-theme-vars",
        "build-pnc": "ng build --prod --base-href=./ && node src/scripts/pick-choose-build.js && gzip -c eds.js > eds.js.gz",
        "prebuild": "npm run generate-colormap-and-theme-vars && npm run build-scss && npm run build-scss-only && npm run build-scss-dark-theme-vars && npm run build-scss-no-css-vars && npm run build-docs",
        "build": "ngcc && npm run gen-components-meta && ng build --configuration production --base-href=./ && cp -R ./apiData ./dist/apiData",
        "postbuild": "npm run bundle-dist",
        "build:local": "npm run build",
        "postbuild:local": "rm -rf node_modules package-lock.json && eds install",
        "prebuild-scss-no-css-vars": "npm run test:generate-colormap && npm run test:generate-theme-vars",
        "build-scss-no-css-vars": "node src/scripts/bundle-css.js",
        "bundle-dist": "node src/scripts/build-dist.js && npm run gzip",
        "build-dev": "npm run build-scss && npm run build-docs && npm run gen-components-meta",
        "watch-dev": "npm run gen-components-meta && ng build --configuration dev --watch",
        "watch-sass": "node src/scripts/watch-scss.js",
        "prewatch-dev": "node src/scripts/fix-directory-watcher.js eds && node node_modules/@gehc-ux/eds-tools/scripts/fix-icon-tmp.js",
        "gen-components-meta": "node src/scripts/generate-components-meta.js",
        "test": "echo \"Run unit tests in respective component repositories. Nothing to test here.\"; exit 0;",
        "test:generate-colormap": "node node_modules/@gehc-ux/eds-tools/tests/generate-color-map.js",
        "test:generate-colormap:force": "node node_modules/@gehc-ux/eds-tools/tests/generate-color-map.js --force-regen",
        "test:generate-theme-vars": "cd node_modules/@gehc-ux/eds-tools && node tests/kickoff-webpack.js && node dist/generate-theme-variables-map",
        "compodoc": "compodoc -p src/tsconfig.app.json",
        "compodoc-serve": "http-server -p 4300 ./documentation",
        "stylelint": "stylelint '**/*.scss' --config src/.stylelintrc.json -f json > coverage/stylelint-report.json; exit 0",
        "stylestats": "stylestats eds.min.css",
        "links:eds2eds-website": "npm link && cd ../eds-website && npm link @gehc-ux/eds",
        "links:npm-build-all": "./src/scripts/utility.sh npmBuildAllLinks",
        "links:npm-relink": "./src/scripts/utility.sh npmLinkAllIntoEds",
        "links:report": "cd ..; find ./**/node_modules/\\@gehc-ux -type l ",
        "links:remove-all": "cd ..; find **/node_modules/\\@gehc-ux -type l -exec rm {} \\;",
        "git:clone-all": "./src/scripts/clone-all.sh",
        "git:pull-all": "./src/scripts/utility.sh gitPullAll",
        "git:report-non-dev": "./src/scripts/utility.sh gitReportNonDevRepoAll",
        "git:report-branches": "./src/scripts/utility.sh gitRepoBranchReportAll",
        "git:prune-merged-branches": "./src/scripts/utility.sh gitPruneMergedBranchesAll",
        "eds:rebirth": "./src/scripts/utility.sh edsRebirth",
        "eds-website:rebirth": "./src/scripts/utility.sh edsWebsiteRebirth",
        "util": "./src/scripts/utility.sh --",
        "fix-memory-limit": "cross-env LIMIT=8192 increase-memory-limit",
        "eds-cicd-add-prerelease-to-dependencies": "eds-cicd-add-prerelease-to-dependencies",
        "eslint": "eslint . --ext .js,.jsx,.ts,.tsx"
    },
    "husky": {"hooks":     {
        "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
        "pre-push": "eds-pre-push-hook"
    }},
